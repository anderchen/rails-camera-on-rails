<h1>Bookings new page</h1>
<%= simple_form_for [@device, @booking] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_date%>
    <%= f.input :end_date%>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>
